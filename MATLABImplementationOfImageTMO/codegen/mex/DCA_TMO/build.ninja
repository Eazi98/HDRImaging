build $builddir/abs.o : cc $root/abs.c
build $builddir/indexShapeCheck.o : cc $root/indexShapeCheck.c
build $builddir/interp1.o : cc $root/interp1.c
build $builddir/svd.o : cc $root/svd.c
build $builddir/imfilter.o : cc $root/imfilter.c
build $builddir/assertCompatibleDims.o : cc $root/assertCompatibleDims.c
build $builddir/ixfun.o : cc $root/ixfun.c
build $builddir/_coder_DCA_TMO_api.o : cc $root/interface/_coder_DCA_TMO_api.c
build $builddir/_coder_DCA_TMO_mex.o : cc $root/interface/_coder_DCA_TMO_mex.c
build $builddir/eml_int_forloop_overflow_check.o : cc $root/eml_int_forloop_overflow_check.c
