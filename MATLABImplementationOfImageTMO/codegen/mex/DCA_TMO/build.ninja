build $builddir/c_mexapi_version.o : cc $matlabdir/extern/version/c_mexapi_version.c

# Link
build $root\DCA_TMO_mex.mexw64 : link $builddir/DCA_TMO_data.o $builddir/rt_nonfinite.o $builddir/DCA_TMO_initialize.o $builddir/DCA_TMO_terminate.o $builddir/DCA_TMO.o $builddir/MaxQuart.o $builddir/power.o $builddir/vAllOrAny.o $builddir/quantizeNL_float.o $builddir/mean.o $builddir/sum.o $builddir/median.o $builddir/quickselect.o $builddir/tvi.o $builddir/abs.o $builddir/indexShapeCheck.o $builddir/interp1.o $builddir/svd.o $builddir/imfilter.o $builddir/assertCompatibleDims.o $builddir/ixfun.o $builddir/_coder_DCA_TMO_api.o $builddir/_coder_DCA_TMO_mex.o $builddir/eml_int_forloop_overflow_check.o $builddir/sumMatrixIncludeNaN.o $builddir/sort.o $builddir/sortIdx.o $builddir/atan.o $builddir/useConstantDim.o $builddir/DCA_TMO_emxutil.o $builddir/_coder_DCA_TMO_info.o $builddir/c_mexapi_version.o 
