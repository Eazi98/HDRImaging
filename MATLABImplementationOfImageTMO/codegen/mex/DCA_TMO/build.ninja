# CompilerName=mingw64
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/win64
matlabdir = C:/Program Files/MATLAB/R2022b
startdir = C:/Users/xuezh/OneDrive - Singapore Institute Of Technology/Desktop/Y3 Trimester 1/Capstone/Files for Capstone/MATLAB implementation of imageTMO

# Toolchain information
cc = C:\ProgramData\MATLAB\SupportPackages\R2022b\3P.instrset\mingw_w64.instrset\bin\gcc
cxx = C:\ProgramData\MATLAB\SupportPackages\R2022b\3P.instrset\mingw_w64.instrset\bin\g++
linker = C:\ProgramData\MATLAB\SupportPackages\R2022b\3P.instrset\mingw_w64.instrset\bin\gcc
linkerxx = C:\ProgramData\MATLAB\SupportPackages\R2022b\3P.instrset\mingw_w64.instrset\bin\g++

cflags =  -c -fexceptions -fno-omit-frame-pointer -m64 -DMATLAB_MEX_FILE  -DMATLAB_MEX_FILE -O2 -fwrapv -DNDEBUG -std=c99 -I "C:\Users\xuezh\OneDrive - Singapore Institute Of Technology\Desktop\Y3 Trimester 1\Capstone\Files for Capstone\MATLAB implementation of imageTMO\codegen\mex\DCA_TMO" -I "C:\Users\xuezh\OneDrive - Singapore Institute Of Technology\Desktop\Y3 Trimester 1\Capstone\Files for Capstone\MATLAB implementation of imageTMO" -I "C:\Program Files\MATLAB\R2022b\extern\include" -I "C:\Users\xuezh\OneDrive - Singapore Institute Of Technology\Desktop\Y3 Trimester 1\Capstone\Files for Capstone\MATLAB implementation of imageTMO\codegen\mex\DCA_TMO\interface" -I "." -DHAVE_LAPACK_CONFIG_H -DLAPACK_COMPLEX_STRUCTURE -DMW_HAVE_LAPACK_DECLS -DMODEL=DCA_TMO_mex -D__USE_MINGW_ANSI_STDIO=1  

cxxflags =  -c -fexceptions -fno-omit-frame-pointer -std=c++11 -m64 -DMATLAB_MEX_FILE  -DMATLAB_MEX_FILE -O2 -fwrapv -DNDEBUG -std=c++11 -I "C:\Users\xuezh\OneDrive - Singapore Institute Of Technology\Desktop\Y3 Trimester 1\Capstone\Files for Capstone\MATLAB implementation of imageTMO\codegen\mex\DCA_TMO" -I "C:\Users\xuezh\OneDrive - Singapore Institute Of Technology\Desktop\Y3 Trimester 1\Capstone\Files for Capstone\MATLAB implementation of imageTMO" -I "C:\Program Files\MATLAB\R2022b\extern\include" -I "C:\Users\xuezh\OneDrive - Singapore Institute Of Technology\Desktop\Y3 Trimester 1\Capstone\Files for Capstone\MATLAB implementation of imageTMO\codegen\mex\DCA_TMO\interface" -I "." -DHAVE_LAPACK_CONFIG_H -DLAPACK_COMPLEX_STRUCTURE -DMW_HAVE_LAPACK_DECLS -DMODEL=DCA_TMO_mex -D__USE_MINGW_ANSI_STDIO=1  

ldflags =  -m64 -Wl,--no-undefined -shared -static -L"C:\Program Files\MATLAB\R2022b\extern\lib\win64\mingw64" -llibmx -llibmex -llibmat -lm -llibmwlapack -llibmwblas -o "DCA_TMO_mex.mexw64" -L"$matlabdir\extern\lib\win64\mingw64" -llibmwlapack -llibmwblas -llibemlrt -llibcovrt -llibut -llibmwmathutil  DCA_TMO_mex.def

linkobjs =  "$matlabdir\extern\lib\win64\mingw64\libmwippfilter.lib" "$matlabdir\extern\lib\win64\mingw64\libmwimfilter.lib"

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker @$out.rsp $linkobjs $ldflags
    rspfile = $out.rsp
    rspfile_content = $in

rule linkxx
    command = $linkerxx $in $linkobjs $ldflags

# Build
build $builddir/DCA_TMO_data.o : cc $root/DCA_TMO_data.c
build $builddir/rt_nonfinite.o : cc $root/rt_nonfinite.c
